				{
					"name": "te-food-bundles.CreateBundle",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Check if the \"bundleID\" variable exists; if not, initialize it with 1 (as an integer)",
									"if (pm.environment.has(\"bundleID\")) {",
									"    // Convert \"bundleID\" from string to integer and then increment by 1",
									"    let currentBundleID = parseInt(pm.environment.get(\"bundleID\"));",
									"    pm.environment.set(\"bundleID\", currentBundleID + 1);",
									"} else {",
									"    // Initialize \"bundleID\" with 1 (as an integer)",
									"    pm.environment.set(\"bundleID\", 1);",
									"}",
									"",
									"// Log the \"bundleID\" environment variable as an integer",
									"console.log(pm.environment.get(\"bundleID\"));"
								],
								"type": "text/javascript"
							}
						}
					],
